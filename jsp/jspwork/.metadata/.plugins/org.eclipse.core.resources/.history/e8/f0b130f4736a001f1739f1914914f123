CREATE TABLE MEMBER 
   (	ID VARCHAR2(200 BYTE), 
	PASSWORD VARCHAR2(200 BYTE) NOT NULL, 
	NAME VARCHAR2(200 BYTE) NOT NULL, 
	GENDER VARCHAR2(200 BYTE), 
	BIRTH VARCHAR2(200 BYTE), 
	MAIL VARCHAR2(200 BYTE), 
	PHONE VARCHAR2(200 BYTE), 
	ADDRESS VARCHAR2(2000 BYTE), 
	REGIST_DAY VARCHAR2(200 BYTE), 
	PRIMARY KEY (ID)
  );
  
  
  