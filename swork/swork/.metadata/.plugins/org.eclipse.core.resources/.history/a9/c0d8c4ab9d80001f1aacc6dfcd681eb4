package com.kr.co;

public class Lg implements TV {

	public Lg() {
		System.out.println("LgTV 객체 생성");
	}
	
	@Override
	public void powerOn() {
		System.out.println("L 전원 켜기");
	}

	@Override
	public void powerOff() {
		System.out.println("L 전원 끄기");
	}

	@Override
	public void volumeUp() {
		
	}

	@Override
	public void volumeDown() {
		
	}
	
}
